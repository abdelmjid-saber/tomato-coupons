{
    "name": "tomatophp/tomato-coupons",
    "type": "library",
    "description": "Offers and coupons, gift cards manager for TomatoPHP e-commerce system.",
    "keywords": [
        "php",
        "offers",
        "coupons",
        "gift cards",
        "discounts",
        "referral codes",
        "laravel",
        "tomatophp",
        "ecommerce"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoCoupons\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoCoupons\\TomatoCouponsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "tomatophp/tomato-products": "^1.0",
        "tomatophp/tomato-category": "^1.1",
        "tomatophp/tomato-locations": "^1.1"
    }
}
